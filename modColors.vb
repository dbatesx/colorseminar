    Function getcolor(ByVal i As Integer, ByVal j As Integer, ByVal k As Integer) As System.Drawing.Color
        Dim red1 As String
        Dim green1 As String
        Dim blue1 As String
        Dim red As Integer = 0
        Dim green As Integer = 0
        Dim blue As Integer = 0

        If mycolors(i, j, k) = "" Then
            initialize_colors()
        End If

        red1 = (Mid(mycolors(i, j, k), 1, 3))
        green1 = (Mid(mycolors(i, j, k), 4, 3))
        blue1 = (Mid(mycolors(i, j, k), 7, 3))

        red = CInt(red1)
        green = CInt(green1)
        blue = CInt(blue1)

        getcolor = System.Drawing.Color.FromArgb(red, green, blue)

    End Function
